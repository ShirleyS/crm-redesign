         <!--    <img id="chart-top" src="http://basicblogtips.com/wp-content/uploads/2012/04/Viewbix-Charts.jpg"> -->
          <img id="chart-top" src="http://basicblogtips.com/wp-content/uploads/2012/04/Viewbix-Charts.jpg">
          </div>
        </div>
        <div class="header-sec-2">    
          
         <!--    <div class="container-module">Open Tasks:&nbsp;&nbsp;&nbsp;&nbsp;39 -->
            <div id="top-con-1" class="container-module mini-mod"><div class="mini-mod-top">Without Card:<br></div><div class="mini-mod-response">260</div></div>
            <div id="top-con-2" class="container-module mini-mod"><div class="mini-mod-top">Current Paid:<br></div><div class="mini-mod-response">500</div></div>
            <div id="top-con-3" class="container-module mini-mod"><div class="mini-mod-top">Expiring Soon:<br></div><div class="mini-mod-response">92</div></div>
            <div id="top-con-4" class="container-module mini-mod"><div class="mini-mod-top">+/- 2 weeks:<br></div><div class="mini-mod-response">80</div></div>
            <br>
            <div id="top-con-5" class="container-module mini-mod"><div class="mini-mod-top">Shopify Signups:<br></div><div class="mini-mod-response">50</div></div>
            <div id="top-con-6" class="container-module mini-mod"><div class="mini-mod-top">Etsy Signups:<br></div><div class="mini-mod-response">60</div></div>
            
            <div id="top-con-7" class="container-module mini-mod"><div class="mini-mod-top">New Tasks:<br></div><div class="mini-mod-response">6</div></div>
            <div id="top-con-8" class="container-module mini-mod"><div class="mini-mod-top">Active Today:<br></div><div class="mini-mod-response">78%</div></div>
      
        </div>  
     

        <div class="header-sec-3">    
          <ul>
            <h2 style="color:#5f6f81">Recently Assigned:</h2>
            <br><br>
            <li><a>Resolve Twilio Double Fires</a></li><br>
            <li><a>Close the Meetup Deal</a></li><br>
            <li><a>Demo the product at Stripe</a></li><br>
            <li><a>Calls from the new lead list</a></li><br>
            <li><a>Fix payment processing delay</a></li><br>
            <li><a>Return Call from Zendesk</a></li><br>
     
          </ul>
        </div>
    </header> 
  </div>


<!-- Accordion Customers -->
  <script type="text/x-handlebars" id="index">
    <div class="menu-div">
      <div class="integration-list">
        <ul>
        <h5>Recent Tickets</h5>
          {{#each tickets}}
          <li>
            <div class="expand">
              <span class="arrow" style="width:5%;float:left;height:100%;" ><strong>+<</strong></span>
              <span class="menu-cell">  
                  <div class="mr-title">{{title}}</div> 
                  <div class="mr-customer-tickets">{{#link-to 'customer' this}} {{customer_name}} {{/link-to}}</div>
               
              <div class="tag-unassigned">unassigned</div>
              <div class="tag-open">OPEN</div>

              </span>
            </div>
              
            <div class="detail">

              <div class="right" style="width:85%;float:right;height:100%;padding-left:20px;">
                <div class="sup">
                  <div>
                   
                    <span> I am getting duplicate texts every time I message Kit. Please help!</span>

                    <br />
                    <br />
                    <a href="#">Run Report</a>
                    <a href="#">Email</a>
                    <a href="#">Text</a>
                    <a href="#">Edit</a>
                    <a href="#">Assign</a>
                  </div>
                </div>
              </div>
            </div>     
          </li>            
          {{/each}}
        </ul>
      </div>

      <div class="integration-list">
      <ul>
        <h5>Recent Signups</h5>
          {{#each customers}}
          <li>
            <div class="expand">
              <span class="arrow" style="width:5%;float:left;height:100%;"><strong>+</strong></span>
                <div class="menu-cell">  
                  <div class="menu-row mr-email" style="text-decoration:none">{{email}}</div>
                  <div class="menu-row mr-customer-signups">{{#link-to 'customer' this}} {{customer_name}} {{/link-to}}</div> 
                  <div class="menu-row mr-date">Signed Up: {{format-date date}}</div>
                  <div class="menu-row mr-trial-type">Type: <div class="mr-type-trial-paid">{{trial_status.type}}</div></div>
                </div>
           

              </div>
              <div class="detail">

                <div class="right" style="width:85%;float:right;height:100%;padding-left:10px;">
                  <div class="sup">
                    <div>
                     
                      <span>Company: Big Co. </span>
                      <span>Location: San Francisco, CA </span>
                      <br>
                      <span>Paid: Yes </span>
                      <span>|Credit Card Valid: Yes </span>
                      <span>|Ads Run: 2 </span>
                      <span>|FB Terms: Yes </span>

                      <br />
                      <br />
                      <a href="#">Run Report</a>
                      <a href="#">Email</a>
                      <a href="#">Text</a>
                      <a href="#">Edit</a>
                      <a href="#">Assign</a>
                    </div>
                  </div>
                </div>
            </div>
          </li>            
          {{/each}}
        </ul>
        
      </div>
    </div>


            
    <div class="span9">
      {{outlet}}
    </div>
  </script>
  <!-- /container -->

