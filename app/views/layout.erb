            </ul>
          </div>
          <!-- /gn-scroller -->
        </nav>
      </li>
     
      <li class="kit-logo"><a href="#">SS. CRM</a></li>
    </ul>
  

    <header>
      
      <!--   <div class="header-sec-1 container-module">
          <div class="chart-text">Recent User Activity:</div>

        </div> -->
        
